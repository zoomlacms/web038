@model ZoomLa.SQLDAL.SQL.PageSetting
@{
Layout = "~/Areas/AdminMVC/Views/Shared/_layTop.cshtml";
}
@section head{
    <title>@Resources.L.课程管理</title>
}
@section content{
    <div id="BreadDiv" class="container-fluid mysite">
        <div class="row">
            <ol class="breadcrumb navbar-fixed-top w-100">
                <li class="breadcrumb-item"><a href='@(CustomerPageAction.customPath2)I/Main.aspx'>@Resources.L.工作台</a></li>
                <li class="breadcrumb-item"><a href='Papers_System_Manage'>@Resources.L.教育模块</a></li>
                <li class="breadcrumb-item"><a href='Papers_System_Manage'>@Resources.L.考试管理</a></li>
                <li class="breadcrumb-item">@Resources.L.课程管理<a href='AddCourse'> [ @Resources.L.添加课程 ]</a></li>
                @MvcHtmlString.Create(Call.GetHelp(75))
            </ol>
        </div>
    </div>
    @Html.Partial("CourseManage_List")
    <div class="alert alert-light">
        <button type="button" class="btn btn-outline-danger" onclick='mvcpage.bat({ url: "Course_Del", confirm: "不可恢复性删除数据,你确定将该数据删除吗？" });'>@Resources.L.批量删除</button>
    </div>
}
@section script{

}